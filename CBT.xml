<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="com.testing.util.TestListener"></listener>
	</listeners>
	<test name="Chrome Test">
		<parameter name="browser-name" value="Chrome"></parameter>
		<classes>
			<class name="com.testing.uitest.homepagetest.SignupPageTests" />
			<class name="com.testing.uitest.HomePageTest" />
			<class name="com.testing.config.BaseClass" />
		</classes>
	</test> <!-- Test -->
	<test name="Firefox Test">
		<parameter name="browser-name" value="Firefox"></parameter>
		<classes>
			<class name="com.testing.uitest.homepagetest.SignupPageTests" />
			<class name="com.testing.uitest.HomePageTest" />
			<class name="com.testing.config.BaseClass" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
